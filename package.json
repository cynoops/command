{
  "name": "command",
  "version": "0.0.2-alpha",
  "main": "index.js",
  "build": {
    "productName": "CYNOOPS Command",
    "appId": "com.cynoops.command",
    "files": [
      "index.js",
      "preload.js",
      "renderer.js",
      "index.html",
      "styles.css",
      "scale-dialog.css",
      "translations.json",
      "assets/**",
      "src/**",
    "dist/vendor/**",
    "node_modules/firebase/**",
    "node_modules/@firebase/**",
    "node_modules/firebase-admin/**"
    ],
    "mac": {
      "icon": "build/icons/mac/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "build/icons/win/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "icon": "build/icons/png",
      "target": [
        "AppImage"
      ]
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "icons": "npx electron-icon-maker --input ./cynoops_app_icon.png --output ./build",
    "prestart": "npm run assets",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "predist": "npm run icons && npm run assets",
    "dist": "electron-builder",
    "release": "electron-builder --publish always",
    "postinstall": "npm-run-all assets install:* symbols:combine",
    "assets": "node ./scripts/copy-mapbox-assets.js",
    "install:icons": "cp -r ./node_modules/@phosphor-icons/core/assets ./assets/icons",
    "install:symbols": "wget https://github.com/jonas-koeritz/Taktische-Zeichen/releases/download/v2.0.0/release.zip -O ./assets/symbols/release.zip && cd ./assets/symbols && unzip -o release.zip && rm release.zip",
    "symbols:json": "node ./scripts/generate-symbols-json.js",
    "symbols:combine": "node ./scripts/generate-symbols-combined.js"
  },
  "keywords": [],
  "author": "Georges Jentgen <georges@cynoops.com>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1",
    "firebase": "^10.12.5",
    "firebase-admin": "^13.6.0",
    "mapbox-gl": "^3.15.0",
    "npm-run-all": "^4.1.5",
    "openai": "^5.23.1"
  },
  "devDependencies": {
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "electron-updater": "^6.6.2"
  }
}
