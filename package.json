{
  "name": "command",
  "version": "0.0.1-alpha",
  "main": "index.js",
  "build": {
    "productName": "Command",
    "appId": "com.cynoops.command",
    "files": [
      "dist/**",
      "index.js",
      "preload.js",
      "index.html",
      "styles.css"
    ],
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "prestart": "npm run assets",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "predist": "npm run assets",
    "dist": "electron-builder",
    "release": "electron-builder --publish always",
    "postinstall": "npm-run-all assets install:*",
    "assets": "node ./scripts/copy-mapbox-assets.js",
    "install:icons": "cp -r ./node_modules/@phosphor-icons/core/assets ./assets/icons"
  },
  "keywords": [],
  "author": "Georges Jentgen <georges@polysign.lu>",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1",
    "mapbox-gl": "^3.15.0",
    "npm-run-all": "^4.1.5",
    "openai": "^5.23.1",
    "serialport": "^13.0.0"
  },
  "devDependencies": {
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "electron-updater": "^6.6.2"
  }
}
