{
  "name": "command",
  "version": "1.0.0",
  "main": "index.js",
  "build": {
    "appId": "com.cynoops.command",
    "files": [
      "dist/**",
      "main.js",
      "preload.js",
      "index.html",
      "styles.css"
    ],
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "serialport": "^12.0.0"
  },
  "devDependencies": {
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "electron-updater": "^6.6.2"
  }
}
